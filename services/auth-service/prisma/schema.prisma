generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int      @id @default(autoincrement())
  googleId      String   @unique
  email         String   @unique
  name          String
  avatar        String?
  createdAt     DateTime @default(now())
}
